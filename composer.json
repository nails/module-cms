{
    "name": "nailsapp/module-cms",
    "keywords": ["Nails", "Shed Collective", "CodeIgniter", "CMS Module"],
    "homepage": "http://nailsapp.co.uk/modules/cms",
    "description": "This is the \"CMS\" module for Nails.",
    "license": "MIT",
    "authors":
    [
        {
            "name": "Pablo de la Pena",
            "email": "p@nailsapp.co.uk",
            "homepage": "http://nailsapp.co.uk",
            "role": "Developer"
        },
        {
            "name": "Gary Duncan",
            "email": "g@nailsapp.co.uk",
            "homepage": "http://nailsapp.co.uk",
            "role": "Developer"
        }
    ],
    "support":
    {
        "email": "hello@nailsapp.co.uk"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require":
    {
        "nailsapp/module-cdn": "dev-develop"
    },
    "autoload":
    {
        "psr-4": {"Nails\\Cms\\": "src/"}
    },
    "extra":
    {
        "nails" :
        {
            "moduleName": "cms",
            "type": "module",
            "autoload": {
                "helpers": ["cms"]
            },
            "data": {
                "nailsapp/module-admin": {
                    "autoload": {
                        "assets": {
                            "js": [
                                ["mustache.js/mustache.js", "NAILS-BOWER"],
                                ["jquery-serialize-object/dist/jquery.serialize-object.min.js", "NAILS-BOWER"],
                                ["admin.widgeteditor.min.js", "nailsapp/module-cms"]
                            ],
                            "css": [
                                ["admin.css", "nailsapp/module-cms"],
                                ["admin.widgeteditor.css", "nailsapp/module-cms"]
                            ]
                        }
                    }
                }
            }
        }
    }
}
